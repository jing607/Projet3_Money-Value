{"ast":null,"code":"import axios from 'axios';\nimport ErrorAlert from './ErrorAlert.vue';\nexport default {\n  name: 'ForgotCard',\n  components: {\n    ErrorAlert\n  },\n  data() {\n    return {\n      email: '',\n      message: '',\n      error: ''\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      try {\n        await axios.post('forgot', {\n          email: this.email\n        });\n        this.message = 'The email was sent!';\n        this.error = '';\n      } catch (e) {\n        this.error = 'Error occured';\n        this.error = '';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","ErrorAlert","name","components","data","email","message","error","methods","handleSubmit","post","e"],"sources":["/Users/hj/Desktop/cours/6_Projet3_Money-Value/Projet3_Money-Value/Admin-Vue/front/src/components/login/ForgotCard.vue"],"sourcesContent":["<template>\n    <div class=\"auth-wrapper\">\n      <div class=\"auth-inner\">\n        <form @submit.prevent=\"handleSubmit\">\n            <div v-if=\"message\" class=\"alert alert-success\" role=\"alert\">\n                {{ message }}\n            </div>\n\n            <ErrorAlert v-if=\"error\" error=\"error\" />\n            <!-- <ErrorAlert :error=\"error\"></ErrorAlert> -->\n\n            <h3>Forgot Password</h3>\n\n            <div class=\"form-group\">\n                <label>Entre the email you registered with.</label>\n                <input type=\"email\" class=\"form-control\" v-model=\"email\" placeholder=\"Email\">\n                <small class=\"text-info\"><em>We will send you an email to reset your password.</em></small>    \n            </div>\n\n            <button class=\"btn btn-primary btn-block mt-3\">Submit</button>\n        </form>\n    </div>\n  </div>\n</template>\n\n\n\n<script>\n    import axios from 'axios'\n    import ErrorAlert from './ErrorAlert.vue';\n\n    export default {\n        name: 'ForgotCard',\n        components: {\n            ErrorAlert\n        },\n        data() {\n            return {\n                email: '',\n                message: '',\n                error: ''\n            }\n        },\n        methods:{\n            async handleSubmit() {\n                try {\n                    await axios.post('forgot', {\n                        email: this.email\n                    });\n\n                    this.message = 'The email was sent!';\n                    this.error = '';\n                } catch (e) {\n                    this.error = 'Error occured';\n                    this.error = '';\n                }\n            }\n        }\n    }\n</script>"],"mappings":"AA4BI,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;IACX;EACJ,CAAC;EACDC,OAAO,EAAC;IACJ,MAAMC,YAAYA,CAAA,EAAG;MACjB,IAAI;QACA,MAAMT,KAAK,CAACU,IAAI,CAAC,QAAQ,EAAE;UACvBL,KAAK,EAAE,IAAI,CAACA;QAChB,CAAC,CAAC;QAEF,IAAI,CAACC,OAAM,GAAI,qBAAqB;QACpC,IAAI,CAACC,KAAI,GAAI,EAAE;MACnB,EAAE,OAAOI,CAAC,EAAE;QACR,IAAI,CAACJ,KAAI,GAAI,eAAe;QAC5B,IAAI,CAACA,KAAI,GAAI,EAAE;MACnB;IACJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}